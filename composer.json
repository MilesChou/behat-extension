{
    "name": "mileschou/behat-extension",
    "description": "The extensions for behat",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "template"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "behat/gherkin": "^4.9",
        "mileschou/codegener": "^1.5"
    },
    "require-dev": {
        "illuminate/filesystem": "^9.13",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Behat\\Extension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
